{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rules": {
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "interface-name": false
    },
    "linterOptions": {
        "exclude": ["**/node_modules/**/*", "**/dist/**/*"]
    }
}
