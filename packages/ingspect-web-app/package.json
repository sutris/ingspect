{
  "private": true,
  "name": "ingspect-web-app",
  "version": "0.1.0",
  "scripts": {
    "build": "react-scripts build && workbox generateSW workbox-config.js",
    "deploy": "npm run build && netlify deploy --dir=build --prod",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test --watchAll=false",
    "test:ci": "CI=1 npm run test -- --coverage --colors --watchAll=false",
    "test:watch": "react-scripts test"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "ingspect-dict": "^0",
    "ingspect-lib": "^0",
    "netlify-cli": "^2.11.8",
    "normalize.css": "^8.0.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "tesseract.js": "^1.0.14"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.5",
    "@types/react": "^16.7.2",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/tesseract.js": "0.0.2",
    "jest-dom": "^3.2.2",
    "react-testing-library": "^7.0.0",
    "workbox-cli": "^4.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "json",
      "text"
    ]
  }
}
